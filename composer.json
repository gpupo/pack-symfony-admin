{
    "name": "gpupo/pack-symfony-admin",
    "description": "Pacote de componentes complementares para dashboards com Symfony Framework",
    "homepage": "https://opensource.gpupo.com/pack-symfony-admin/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["common", "development"],
    "license": "MIT",
    "require": {
      "gpupo/pack-symfony-common": "^4.5 || dev-master",
      "doctrine/phpcr-bundle": "^2.1 || dev-php-8",
        "aws/aws-sdk-php-symfony": "^2.1",
        "easycorp/easyadmin-bundle": "^2.3.5",
        "league/flysystem-aws-s3-v3": "^1.0",
        "oneup/flysystem-bundle": "^3.4"
    },
    "repositories": [
        {
          "type": "vcs",
          "url": "https://github.com/gpupo/StofDoctrineExtensionsBundle"
        }
     ],
    "autoload": {
        "psr-4": {
            "Gpupo\\PackSymfonyAdmin\\": "src",
            "Gpupo\\PackSymfonyAdmin\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "gpupo/pack-symfony-dev": "dev-master"
    },
    "scripts": {
        "test": "APP_ENV=test vendor/bin/simple-phpunit"
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
